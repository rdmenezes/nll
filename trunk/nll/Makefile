OBJ=configuration.o fft.o indent.o math.o math-floor.o nll.o optimizer-harmony-search.o optimizer-harmony-search-memory.o optimizer-parameters.o potential-gaussian-canonical.o svm.o svm-linear.o

.cpp.o: $(DEPPS)
	$(CC) $(FLAGS) -c -o $@ $<

all: $(OBJ)
	cd .. && mkdir -p $(MODE) && mkdir -p $(MODE)/$(PLATFORM)
	$(CC) -shared -o $(LIBNAME) $(OBJ) $(LIBS)
	mv $(LIBNAME) ../$(LIBPATH)

.PHONY: clean


clean:
	rm -rf *.o *~ core *~ ../$(LIBPATH)/$(LIBNAME) docs